{
  "name": "webpack5_vue3_starter",
  "version": "1.0.0",
  "author": "Eric Chen <eric@ericchenart.com>",
  "repository": "Knight174/webpack5_vue3_starter",
  "license": "MIT",
  "keywords": [
    "webpack",
    "vue3",
    "typescript",
    "start template"
  ],
  "scripts": {
    "dev": "webpack server --mode=development --progress",
    "build": "webpack --mode=production --progress",
    "build:dll": "webpack --config ./build/webpack.dll.config.js --progress",
    "prepare": "husky install",
    "commit": "cz",
    "generateAnalyzFile": "webpack --profile --json > stats.json",
    "analyz": "webpack-bundle-analyzer --port 8888 ./stats.json"
  },
  "lint-staged": {
    "*.{vue,js,ts}": "eslint --fix"
  },
  "dependencies": {
    "@babel/runtime": "^7.18.6",
    "@babel/runtime-corejs3": "^7.18.6",
    "axios": "^0.27.2",
    "core-js": "^3.23.3",
    "element-plus": "^2.2.7",
    "regenerator-runtime": "^0.13.9",
    "vue": "^3.2.37",
    "vue-router": "^4.0.16",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@babel/core": "^7.18.6",
    "@babel/plugin-transform-runtime": "^7.18.6",
    "@babel/preset-env": "^7.18.6",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@swc/core": "^1.2.211",
    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "ajv": "^8.11.0",
    "ajv-keywords": "^3.5.2",
    "autoprefixer": "^10.4.7",
    "babel-loader": "^8.2.5",
    "commitizen": "^4.2.4",
    "css-loader": "^6.7.1",
    "cz-conventional-changelog": "^3.3.0",
    "cz-customizable": "^6.9.0",
    "eslint": "^8.19.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.1.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "mini-css-extract-plugin": "^2.6.1",
    "postcss": "^8.4.14",
    "postcss-loader": "^7.0.0",
    "postcss-preset-env": "^7.7.2",
    "postcss-scss": "^4.0.4",
    "prettier": "^2.7.1",
    "sass": "^1.53.0",
    "sass-loader": "^13.0.1",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.2.3",
    "terser-webpack-plugin": "^5.3.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3",
    "unplugin-auto-import": "^0.9.2",
    "unplugin-vue-components": "^0.21.0",
    "vue-loader": "^17.0.0",
    "vue-tsc": "^0.34.17",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2",
    "webpack-merge": "^5.8.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
